6 constant b/glyph

: >upper ( char -- CHAR )
  dup [char] a [char] z within if [char] a - [char] A + then
;

: glyph ( n -- addr )
  \ assumes ascii
  \ if outside
  dup 32 [char] ~ within not if [char] ? then
  >upper

  dup [char] a < if
    32 - b/glyph * base_chars +
  else
    123 - b/glyph * end_chars +
  then
;



: list_glyph ( addr -- )
  b/glyph 0 do
    dup I + C@ .bin8 cr
  loop
;


